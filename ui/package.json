{
  "name": "gefyra-docker-desktop-extension",
  "version": "1.2.1",
  "private": true,
  "dependencies": {
    "@docker/docker-mui-theme": "^0.0.9",
    "@docker/extension-api-client": "^0.3.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@mui/x-data-grid": "^6.0.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react": "^7.47.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "cra-template": "1.2.0",
    "gefyra": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix && tsc",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@docker/extension-api-client-types": "^0.3.4",
    "@sentry/vite-plugin": "^0.4.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.8"
  }
}
